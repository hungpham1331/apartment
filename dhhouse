                description: "Căn hộ hiện đại tại trung tâm thành phố, gần các trung tâm thương mại. Nội thất đầy đủ, tiện nghi cao cấp.",
                images: [
                    { color: "#F59E0B" }, // Amber
                    { color: "#3B82F6" }, // Blue
                    { color: "#10B981" }  // Green
                ]
            }
        ];

        // Generate more apartments to reach 50
        const allApartments = [...apartments];
        const districts = ["son-tra", "hai-chau", "ngu-hanh-son", "thanh-khe", "lien-chieu"];
        const amenitiesList = ["pool", "gym", "parking", "security", "elevator", "balcony", "wifi", "ac"];
        
        for (let i = 16; i <= 50; i++) {
            const baseApartment = apartments[Math.floor(Math.random() * apartments.length)];
            const randomDistrict = districts[Math.floor(Math.random() * districts.length)];
            const randomBedrooms = Math.floor(Math.random() * 3) + 1;
            const randomBathrooms = Math.min(randomBedrooms, Math.floor(Math.random() * 2) + 1);
            const randomArea = 45 + Math.floor(Math.random() * 80);
            const randomPrice = 6000000 + Math.floor(Math.random() * 16000000);
            
            // Select random amenities
            const randomAmenities = [];
            const amenitiesCount = Math.floor(Math.random() * amenitiesList.length) + 3;
            for (let j = 0; j < amenitiesCount; j++) {
                const amenity = amenitiesList[Math.floor(Math.random() * amenitiesList.length)];
                if (!randomAmenities.includes(amenity)) {
                    randomAmenities.push(amenity);
                }
            }
            
            allApartments.push({
                id: i,
                name: `${baseApartment.name} ${i}`,
                address: `${Math.floor(Math.random() * 100) + 1} ${baseApartment.address.split(',')[0]}, ${randomDistrict === 'son-tra' ? 'Sơn Trà' : randomDistrict === 'hai-chau' ? 'Hải Châu' : randomDistrict === 'ngu-hanh-son' ? 'Ngũ Hành Sơn' : randomDistrict === 'thanh-khe' ? 'Thanh Khê' : 'Liên Chiểu'}, Đà Nẵng`,
                price: randomPrice,
                area: randomArea,
                bedrooms: randomBedrooms,
                bathrooms: randomBathrooms,
                district: randomDistrict,
                amenities: randomAmenities,
                description: baseApartment.description,
                images: baseApartment.images
            });
        }

        // Format price to VND
        function formatPrice(price) {
            return new Intl.NumberFormat('vi-VN').format(price) + ' đ/tháng';
        }

        // Format price range for filter
        function getPriceRange(price) {
            if (price < 5000000) return '0-5';
            if (price < 10000000) return '5-10';
            if (price < 15000000) return '10-15';
            if (price < 20000000) return '15-20';
            return '20+';
        }

        // Get amenity icon
        function getAmenityIcon(amenity) {
            switch(amenity) {
                case 'pool': return '<i class="fas fa-swimming-pool"></i> Hồ bơi';
                case 'gym': return '<i class="fas fa-dumbbell"></i> Phòng gym';
                case 'parking': return '<i class="fas fa-parking"></i> Bãi đỗ xe';
                case 'security': return '<i class="fas fa-shield-alt"></i> An ninh 24/7';
                case 'elevator': return '<i class="fas fa-arrow-alt-circle-up"></i> Thang máy';
                case 'balcony': return '<i class="fas fa-door-open"></i> Ban công';
                case 'wifi': return '<i class="fas fa-wifi"></i> Wifi';
                case 'ac': return '<i class="fas fa-snowflake"></i> Điều hòa';
                default: return '';
            }
        }

        // Render apartments
        function renderApartments(apartmentsToRender) {
            const grid = document.getElementById('apartments-grid');
            grid.innerHTML = '';
            
            if (apartmentsToRender.length === 0) {
                grid.innerHTML = `
                    <div class="col-span-3 text-center py-12">
                        <svg xmlns="http://www.w3.org/2000/svg" class="h-16 w-16 text-gray-400 mx-auto mb-4" fill="none" viewBox="0 0 24 24" stroke="currentColor">
                            <path stroke-linecap="round" stroke-linejoin="round" stroke-width="1" d="M9.172 16.172a4 4 0 015.656 0M9 10h.01M15 10h.01M21 12a9 9 0 11-18 0 9 9 0 0118 0z" />
                        </svg>
                        <h3 class="text-xl font-bold text-gray-700 mb-2">Không tìm thấy căn hộ nào</h3>
                        <p class="text-gray-500">Vui lòng thử lại với các bộ lọc khác</p>
                    </div>
                `;
                return;
            }
            
            apartmentsToRender.forEach(apartment => {
                const card = document.createElement('div');
                card.className = 'apartment-card bg-white rounded-lg overflow-hidden shadow-md hover:shadow-xl transition duration-300';
                
                // Create image slider
                let imagesHTML = `
                    <div class="relative h-56 overflow-hidden">
                        <div class="absolute inset-0 flex">
                `;
                
                apartment.images.forEach((image, index) => {
                    imagesHTML += `
                        <div class="w-full h-full flex-shrink-0" style="background-color: ${image.color};">
                            <div class="w-full h-full flex items-center justify-center text-white">
                                <svg xmlns="http://www.w3.org/2000/svg" class="h-16 w-16" fill="none" viewBox="0 0 24 24" stroke="currentColor">
                                    <path stroke-linecap="round" stroke-linejoin="round" stroke-width="1" d="M3 9l9-7 9 7v11a2 2 0 01-2 2H5a2 2 0 01-2-2V9z" />
                                    <path stroke-linecap="round" stroke-linejoin="round" stroke-width="1" d="M9 22V12h6v10" />
                                </svg>
                            </div>
                        </div>
                    `;
                });
                
                imagesHTML += `
                        </div>
                        <div class="absolute top-4 right-4 bg-white rounded-full px-3 py-1 text-sm font-medium text-gray-700 shadow-md">
                            ${apartment.bedrooms} <i class="fas fa-bed text-gold ml-1 mr-2"></i> ${apartment.bathrooms} <i class="fas fa-bath text-gold ml-1"></i>
                        </div>
                        <div class="absolute bottom-4 left-4 price-tag text-white px-3 py-1 rounded-md font-medium shadow-md">
                            ${formatPrice(apartment.price)}
                        </div>
                    </div>
                `;
                
                // Create apartment info
                const infoHTML = `
                    <div class="p-6">
                        <h3 class="text-xl font-bold mb-2 text-gray-800">${apartment.name}</h3>
                        <p class="text-gray-600 mb-4 flex items-start">
                            <i class="fas fa-map-marker-alt text-gold mt-1 mr-2"></i>
                            ${apartment.address}
                        </p>
                        <div class="flex items-center justify-between mb-4">
                            <span class="text-gray-700"><i class="fas fa-ruler-combined text-gold mr-2"></i>${apartment.area} m²</span>
                        </div>
                        <div class="flex flex-wrap gap-2 mb-4">
                            ${apartment.amenities.slice(0, 3).map(amenity => 
                                `<span class="bg-gray-100 text-gray-700 text-xs px-2 py-1 rounded-md">${getAmenityIcon(amenity)}</span>`
                            ).join('')}
                            ${apartment.amenities.length > 3 ? `<span class="bg-gray-100 text-gray-700 text-xs px-2 py-1 rounded-md">+${apartment.amenities.length - 3}</span>` : ''}
                        </div>
                        <button class="view-details w-full bg-gold hover:bg-darkgold text-white py-2 rounded-md transition duration-300 font-medium" data-id="${apartment.id}">
                            Xem chi tiết
                        </button>
                    </div>
                `;
                
                card.innerHTML = imagesHTML + infoHTML;
                grid.appendChild(card);
            });
            
            // Add event listeners to view details buttons
            document.querySelectorAll('.view-details').forEach(button => {
                button.addEventListener('click', function() {
                    const apartmentId = parseInt(this.getAttribute('data-id'));
                    showApartmentDetails(apartmentId);
                });
            });
            
            // Update results count
            document.getElementById('results-count').innerHTML = `Hiển thị <span class="font-medium">${apartmentsToRender.length}</span> căn hộ`;
        }

        // Show apartment details in modal
        function showApartmentDetails(apartmentId) {
            const apartment = allApartments.find(apt => apt.id === apartmentId);
            if (!apartment) return;
            
            const modalContent = document.getElementById('modal-content');
            
            // Create image slider
            let imagesHTML = `
                <div class="relative h-72 overflow-hidden">
                    <div class="absolute inset-0 flex">
            `;
            
            apartment.images.forEach((image, index) => {
                imagesHTML += `
                    <div class="w-full h-full flex-shrink-0" style="background-color: ${image.color};">
                        <div class="w-full h-full flex items-center justify-center text-white">
                            <svg xmlns="http://www.w3.org/2000/svg" class="h-24 w-24" fill="none" viewBox="0 0 24 24" stroke="currentColor">
                                <path stroke-linecap="round" stroke-linejoin="round" stroke-width="1" d="M3 9l9-7 9 7v11a2 2 0 01-2 2H5a2 2 0 01-2-2V9z" />
                                <path stroke-linecap="round" stroke-linejoin="round" stroke-width="1" d="M9 22V12h6v10" />
                            </svg>
                        </div>
                    </div>
                `;
            });
            
            imagesHTML += `
                    </div>
                    <div class="absolute bottom-4 left-4 price-tag text-white px-4 py-2 rounded-md font-medium shadow-md text-lg">
                        ${formatPrice(apartment.price)}
                    </div>
                </div>
            `;
            
            // Create apartment details
            const detailsHTML = `
                <div class="p-6">
                    <h2 class="text-2xl font-bold mb-2 text-gray-800">${apartment.name}</h2>
                    <p class="text-gray-600 mb-6 flex items-start">
                        <i class="fas fa-map-marker-alt text-gold mt-1 mr-2"></i>
                        ${apartment.address}
                    </p>
                    
                    <div class="grid grid-cols-1 md:grid-cols-3 gap-4 mb-6">
                        <div class="bg-gray-50 p-4 rounded-lg text-center">
                            <p class="text-gray-500 text-sm">Diện tích</p>
                            <p class="text-xl font-bold text-gray-800">${apartment.area} m²</p>
                        </div>
                        <div class="bg-gray-50 p-4 rounded-lg text-center">
                            <p class="text-gray-500 text-sm">Phòng ngủ</p>
                            <p class="text-xl font-bold text-gray-800">${apartment.bedrooms}</p>
                        </div>
                        <div class="bg-gray-50 p-4 rounded-lg text-center">
                            <p class="text-gray-500 text-sm">Phòng tắm</p>
                            <p class="text-xl font-bold text-gray-800">${apartment.bathrooms}</p>
                        </div>
                    </div>
                    
                    <div class="mb-6">
                        <h3 class="text-lg font-bold mb-3 text-gray-800">Mô tả</h3>
                        <p class="text-gray-600">${apartment.description}</p>
                    </div>
                    
                    <div class="mb-6">
                        <h3 class="text-lg font-bold mb-3 text-gray-800">Tiện ích</h3>
                        <div class="grid grid-cols-2 md:grid-cols-4 gap-3">
                            ${apartment.amenities.map(amenity => 
                                `<div class="bg-gray-50 p-3 rounded-lg flex items-center">
                                    <span class="text-gold mr-2">${getAmenityIcon(amenity).split(' ')[0]}</span>
                                    <span class="text-gray-700">${getAmenityIcon(amenity).split(' ').slice(1).join(' ')}</span>
                                </div>`
                            ).join('')}
                        </div>
                    </div>
                    
                    <div class="flex flex-col md:flex-row gap-4">
                        <button class="flex-1 bg-gold hover:bg-darkgold text-white py-3 rounded-md transition duration-300 font-medium">
                            <i class="fas fa-phone-alt mr-2"></i> Liên hệ ngay
                        </button>
                        <button class="flex-1 border border-gold text-gold hover:bg-gold hover:text-white py-3 rounded-md transition duration-300 font-medium">
                            <i class="far fa-calendar-alt mr-2"></i> Đặt lịch xem
                        </button>
                    </div>
                </div>
            `;
            
            modalContent.innerHTML = imagesHTML + detailsHTML;
            document.getElementById('apartment-modal').classList.remove('hidden');
        }

        // Close modal
        document.getElementById('close-modal').addEventListener('click', function() {
            document.getElementById('apartment-modal').classList.add('hidden');
        });

        // Filter apartments
        function filterApartments() {
            const priceFilter = document.getElementById('price-filter').value;
            const bedroomFilter = document.getElementById('bedroom-filter').value;
            const areaFilter = document.getElementById('area-filter').value;
            const searchInput = document.getElementById('search-input').value.toLowerCase();
            
            // Get selected amenities
            const selectedAmenities = Array.from(document.querySelectorAll('.amenity-filter.filter-active'))
                .map(el => el.getAttribute('data-amenity'));
            
            // Filter apartments
            let filteredApartments = allApartments.filter(apartment => {
                // Price filter
                if (priceFilter !== 'all') {
                    const [min, max] = priceFilter.split('-');
                    if (max === '+') {
                        if (apartment.price < parseInt(min) * 1000000) return false;
                    } else {
                        if (apartment.price < parseInt(min) * 1000000 || apartment.price > parseInt(max) * 1000000) return false;
                    }
                }
                
                // Bedroom filter
                if (bedroomFilter !== 'all') {
                    if (bedroomFilter === '4+') {
                        if (apartment.bedrooms < 4) return false;
                    } else {
                        if (apartment.bedrooms !== parseInt(bedroomFilter)) return false;
                    }
                }
                
                // Area filter
                if (areaFilter !== 'all' && apartment.district !== areaFilter) return false;
                
                // Search filter
                if (searchInput && !apartment.name.toLowerCase().includes(searchInput) && 
                    !apartment.address.toLowerCase().includes(searchInput)) return false;
                
                // Amenities filter
                if (selectedAmenities.length > 0) {
                    for (const amenity of selectedAmenities) {
                        if (!apartment.amenities.includes(amenity)) return false;
                    }
                }
                
                return true;
            });
            
            // Sort apartments
            const sortOption = document.getElementById('sort-options').value;
            switch(sortOption) {
                case 'price-asc':
                    filteredApartments.sort((a, b) => a.price - b.price);
                    break;
                case 'price-desc':
                    filteredApartments.sort((a, b) => b.price - a.price);
                    break;
                case 'newest':
                    filteredApartments.sort((a, b) => b.id - a.id);
                    break;
                default:
                    // Default sorting (recommended)
                    break;
            }
            
            renderApartments(filteredApartments);
        }

        // Event listeners for filters
        document.getElementById('price-filter').addEventListener('change', filterApartments);
        document.getElementById('bedroom-filter').addEventListener('change', filterApartments);
        document.getElementById('area-filter').addEventListener('change', filterApartments);
        document.getElementById('search-input').addEventListener('input', filterApartments);
        document.getElementById('sort-options').addEventListener('change', filterApartments);
        
        // Amenity filters
        document.querySelectorAll('.amenity-filter').forEach(button => {
            button.addEventListener('click', function() {
                this.classList.toggle('filter-active');
                filterApartments();
            });
        });

        // Mobile menu toggle
        document.getElementById('mobile-menu-button').addEventListener('click', function() {
            const mobileMenu = document.getElementById('mobile-menu');
            mobileMenu.classList.toggle('hidden');
        });

        // Initialize
        document.addEventListener('DOMContentLoaded', function() {
            renderApartments(allApartments);
        });
    </script>
